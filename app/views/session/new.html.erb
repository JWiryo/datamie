<div class="login">
  <div class="container">
    <div class="form">

    <h1>Login</h1>

    <%= form_for :session, url: url_for(:controller => "session", :action => "create") do |f| %>
      <%= f.text_field :username, :placeholder => "Username" %>
      <%= f.password_field :password, :placeholder => "Password" %>
      <%= f.submit "Login", class: "btn-submit" %>
    <% end %>


    </div>
  </div>
</div>

