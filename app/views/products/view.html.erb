<h1> Products </h1>

<% client = Mysql2::Client.new(:database => "datamie", :host => "localhost", :username => "root", :password => "") %>
<% results = client.query("SELECT * FROM products") %>

<ol>
<% results.each do |row| %>
  <% row.each do |key, value| %>
	<% if key=='Product_Name'%>
	  <li><%= value %></li>
	<% end %>
  <% end %>
<% end %>
</ol>
